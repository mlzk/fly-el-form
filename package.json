{
	"name": "fly-el-form",
	"version": "0.0.15",
	"private": false,
	"author": "flycat",
	"type": "module",
	"description": "A Vue 3 library for building dynamic forms with Element Plus",
	"main": "lib/fly-el-form.umd.js",
	"module": "lib/fly-el-form.es.js",
	"files": [
		"lib"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "hhttps://github.com/mlzk/fly-el-form.git"
	},
	"bugs": {
		"url": "hhttps://github.com/mlzk/fly-el-form/issues"
	},
	"homepage": "hhttps://github.com/mlzk/fly-el-form#readme",
	"keywords": [
		"element",
		"elementPlus",
		"form",
		"vue"
	],
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"peerDependencies": {
		"@element-plus/icons-vue": "^2.3.1",
		"element-plus": "^2.8.8",
		"vue": "^3.0.0"
	},
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"build:lib": "vite build --config vite.lib.config.ts",
		"docs:build": "npm run register:components && vitepress build docs --base /fly-el-form/",
		"preview": "vite preview",
		"test:unit": "vitest",
		"build-only": "vite build",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --fix",
		"format": "prettier --write src/",
		"docs:dev": "vitepress dev docs",
		"register:components": "vitepress-rc",
		"docs:preview": "vitepress preview docs"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.3.1",
		"@vueuse/core": "^11.3.0",
		"element-plus": "^2.8.8",
		"sass": "^1.81.0",
		"vue": "^3.5.12",
		"vue-router": "^4.4.5"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.0",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.9.0",
		"@vitejs/plugin-vue": "^5.1.4",
		"@vitejs/plugin-vue-jsx": "^4.0.1",
		"@vitest/eslint-plugin": "1.1.7",
		"@vue/eslint-config-prettier": "^10.1.0",
		"@vue/eslint-config-typescript": "^14.1.3",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.5.1",
		"eslint": "^9.14.0",
		"eslint-plugin-vue": "^9.30.0",
		"jsdom": "^25.0.1",
		"npm-run-all2": "^7.0.1",
		"prettier": "^3.3.3",
		"terser": "^5.36.0",
		"typescript": "~5.6.3",
		"vite": "^5.4.10",
		"vite-plugin-vue-devtools": "^7.5.4",
		"vitepress": "^1.5.0",
		"vitepress-theme-demoblock": "^3.0.7",
		"vitest": "^2.1.4",
		"vue-tsc": "^2.1.10"
	}
}